<?xml version="1.0" encoding="utf-8" ?>
<configuration>
	<location path="images">
		<system.webServer>
			<directoryBrowse enabled="false" />
		</system.webServer>
	</location>
  <system.web>
    <sessionState 
      mode ="InProc" 
      cookieless="false" 
      timeout="1440"/>
    <authentication mode="Forms">
          <forms timeout="1440"/>
    </authentication>
	<httpRuntime executionTimeout="360" />
  </system.web>
</configuration>